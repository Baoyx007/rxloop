{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "browsers": [
            "ie >= 11"
          ]
        },
        "exclude": ["transform-async-to-generator", "transform-regenerator"],
        "modules": false,
        "loose": true
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread"
  ],
  "env": {
    "commonjs": {
      "presets": [
        [
          "@babel/env",
          {
            "loose": true
          }
        ]
      ]
    }
  }
}